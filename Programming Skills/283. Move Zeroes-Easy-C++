class Solution {
public:
    void moveZeroes(vector<int>& n)
    {
        int s = 0;
        int f = 0;
        //cout<<1;
        while(f<n.size())
        {
            //cout<<f<<s<<n[f]<<n[s]<<" ";
            if(n[f] != 0)
            {
                swap(n[f],n[s]);
                s++;
            }
            f++;
        }
       
    }
};