class Solution 
{
    public boolean isValidSudoku(char[][] board) 
    {
        for(int i = 0 ;i< 9;i++)
        {
            for(int j = 0;j<9;j++)
            {
                if(board[i][j] != '.')
                {
                    if(check(board,i,j) == false)
                        {//System.out.print(board[i][j]+" "+i+" "+j);
                        return false;}
                }
                    
            }
        }
        return true;
    }
    public boolean check(char[][] board, int i,int j)
    {
        
        char var = board[i][j];
        for(int k =0;k<9;k++)
        {
            if(k != i && board[k][j] == var)
            {
                //System.out.print(1);
                return false;
            }
                
        }
        for(int k =0; k <9;k++)
        {
            if(k!=j && board[i][k] == var)
            {
                //System.out.print(2);
                return false;
            }
        }
        
        for(int k = (i/3)*3; k<(i/3)*3 + 3; k++)
        {
            for(int l = (j/3)*3;l<(j/3)*3 +3;l++)
            {
                if(k !=i && l != j && board[k][l] == var)
                {
                    //System.out.println(3+" "+k+l);
                    return false;
                }
            }
        }
        return true;
    }
}
