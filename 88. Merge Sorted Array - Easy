class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) 
    {
        List<Integer> list = new LinkedList<>();
        int j = n-1,i = m-1, k = m+n-1;
        //System.out.print(i+" "+j+" "+k+" ");
        while(i>=0 && j>=0)
        {
            if(nums1[i] <= nums2[j])
            {
                nums1[k--] = nums2[j--];
            }
            
            else if(nums1[i]> nums2[j])
            {
                nums1[k--] = nums1[i--];
            }
                 
        }
        while(j>=0)
        {
            nums1[k--] = nums2[j--];
        }
        
    }
}
