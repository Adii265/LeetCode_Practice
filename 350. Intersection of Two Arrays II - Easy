class Solution:
    def intersect(self, nums1: List[int], nums2: List[int]) -> List[int]:
        d = {}
        for i in nums1:
            if i in d:
                d[i][0] = d[i][0]+1
            else:
                d[i] = [1,0]
        for i in nums2:
            if i in d:
                d[i][1] = d[i][1]+1
            else:
                d[i] = [0,1]
                
        #print(d)
        ans = []
        for i in d:
            if(min(d[i][0],d[i][1])) >0:
                for j in range(min(d[i][0],d[i][1])):
                    ans.append(i)
                
        return ans
